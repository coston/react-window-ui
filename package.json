{
  "name": "react-window-ui",
  "version": "2.1.0",
  "description": "React components for fast presentational window ui",
  "main": "dist/index.js",
  "author": "Coston Perkins <coston.perkins@ua.edu> (https://coston.io)",
  "homepage": "https://github.com/coston/react-window-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coston/react-window-ui.git"
  },
  "bugs": {
    "url": "https://github.com/coston/react-window-ui/issues"
  },
  "license": "GNU General Public License v3.0",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "build": "babel -d dist src",
    "clean": "rm -rf dist && mkdir dist",
    "watch": "babel -w -d dist src",
    "preversion": "npm run clean && npm run build",
    "postpublish": "git push && git push --tag",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^24.7.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.7.1",
    "next": "^9.0.2",
    "prettier": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6"
  },
  "keywords": [
    "react",
    "react-component",
    "window",
    "ui",
    "terminal",
    "browser window",
    "window ui",
    "mac terminal",
    "mac window",
    "OS X window"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "styled-components": "^5.0.1"
  }
}
